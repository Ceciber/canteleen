<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg" style="background:white">
  <!-- Contour principal -->
  <rect x="10" y="10" width="780" height="580" stroke="black" stroke-width="2" fill="none"/>

  <!-- Zone cuisine/service -->
  <rect x="40" y="40" width="460" height="400" stroke="black" stroke-width="2" fill="none"/>

  <!-- Zones 1 à 5 -->
  <rect x="60" y="60" width="50" height="120" fill="#f9f3e3" stroke="black"/>
  <text x="75" y="125" font-size="20" text-anchor="middle" fill="brown">1</text>

  <rect x="120" y="60" width="50" height="120" fill="#f9f3e3" stroke="black"/>
  <text x="135" y="125" font-size="20" text-anchor="middle" fill="brown">2</text>

  <rect x="240" y="40" width="80" height="30" fill="#f9f3e3" stroke="black"/>
  <text x="280" y="60" font-size="20" text-anchor="middle" fill="brown">3</text>

  <rect x="350" y="60" width="50" height="120" fill="#f9f3e3" stroke="black"/>
  <text x="375" y="125" font-size="20" text-anchor="middle" fill="brown">4</text>

  <circle cx="260" cy="170" r="40" fill="#f9f3e3" stroke="black"/>
  <text x="260" y="175" font-size="20" text-anchor="middle" fill="brown">5</text>

  <!-- Couverts/verres -->
  <rect x="30" y="200" width="30" height="120" fill="#e0f7ff" stroke="black"/>
  <text x="45" y="260" font-size="10" text-anchor="middle" transform="rotate(-90 45,260)" fill="blue">cutlery and glasses</text>

  <!-- Écrans -->
  <rect x="70" y="330" width="10" height="20" fill="#cccccc" stroke="black"/>
  <rect x="85" y="330" width="10" height="20" fill="#cccccc" stroke="black"/>
  <text x="75" y="365" font-size="10" fill="blue">screens</text>

  <!-- Caisse -->
  <rect x="440" y="220" width="20" height="60" fill="#fff8dc" stroke="black"/>
  <circle cx="450" cy="230" r="3" fill="black"/>
  <circle cx="450" cy="240" r="3" fill="black"/>
  <text x="420" y="300" font-size="14" fill="skyblue">Cashier</text>

  <!-- Zone tables -->
  <g transform="translate(520, 60)">
    <!-- 4 colonnes x 4 lignes -->
    <g>
      <!-- 4 rangées -->
      <g>
        <rect x="0" y="0" width="80" height="40" fill="#fff" stroke="black"/>
        <rect x="0" y="60" width="80" height="40" fill="#fff" stroke="black"/>
        <rect x="0" y="120" width="80" height="40" fill="#fff" stroke="black"/>
        <rect x="0" y="180" width="80" height="40" fill="#fff" stroke="black"/>
      </g>
      <!-- 4 colonnes -->
      <g transform="translate(100,0)">
        <rect x="0" y="0" width="80" height="40" fill="#fff" stroke="black"/>
        <rect x="0" y="60" width="80" height="40" fill="#fff" stroke="black"/>
        <rect x="0" y="120" width="80" height="40" fill="#fff" stroke="black"/>
        <rect x="0" y="180" width="80" height="40" fill="#fff" stroke="black"/>
      </g>
    </g>
  </g>

  <!-- Entrée -->
  <path d="M500,30 l10,0 l0,20" stroke="black" stroke-width="3"/>
  <text x="500" y="25" font-size="12">Entrance</text>

  <!-- Légende -->
  <g transform="translate(60,480)">
    <text x="0" y="0" font-size="16" fill="brown">1.</text>
    <text x="20" y="0" font-size="16">Starters</text>

    <text x="0" y="25" font-size="16" fill="brown">2.</text>
    <text x="20" y="25" font-size="16">Meat</text>

    <text x="0" y="50" font-size="16" fill="brown">3.</text>
    <text x="20" y="50" font-size="16">Vegetarian</text>

    <text x="0" y="75" font-size="16" fill="brown">4.</text>
    <text x="20" y="75" font-size="16">Fish</text>

    <text x="0" y="100" font-size="16" fill="brown">5.</text>
    <text x="20" y="100" font-size="16">Dessert</text>
  </g>
</svg>
